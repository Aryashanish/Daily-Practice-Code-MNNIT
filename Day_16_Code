//Complete Code
#include<bits/stdc++.h>
#include <iostream>
using namespace std;
#define ll long long

int main()

{

ll t;

cin>>t;

while(t--)

{

ll num;

cin>>num;

string str=to_string(num);

size_t found = str.find("21");

if(found != string::npos)

{

cout<<"The streak is broken!"<<endl;

}

else if(num%21==0)

{

cout<<"The streak is broken!"<<endl;

}

else

{

cout<<"The streak lives still in our heart!"<<endl;

}

}

return 0;

}
