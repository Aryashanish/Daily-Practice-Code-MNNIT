//Problem Link
https://leetcode.com/problems/find-peak-element/?fbclid=IwAR0pQwX_I-0BaCA2DJ6VKFIDB95vnJRwRsO6FiOk9Fyt3NCywPJgDuYLE5k

//Complete Function
int findPeakElement(vector<int>& nums) {
        int x = -1;
        int max = nums[0];
        for(int i=0; i<nums.size(); i++){
            if(nums[i] >= max){
                x = i;
                max = nums[i];
            }
        }
        return x;
 }
 
 Time Complexity - O(n)
